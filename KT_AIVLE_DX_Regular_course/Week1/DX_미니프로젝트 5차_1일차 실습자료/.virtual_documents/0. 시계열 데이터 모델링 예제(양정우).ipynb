def show_trend(col, start, end='9999-12-31'):
    temp = data.loc[data['DateTime'].between(start, end)]
    plt.figure(figsize=(10, 2))
    plt.plot('DateTime', col, data=temp, linewidth=0.8)
    plt.title(col, size=12, pad=10)
    plt.tight_layout()
    plt.show()





