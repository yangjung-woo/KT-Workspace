def hello(name, loud=1):
    if loud == 1:
        return 'HELLO '+ name.upper()+"!!!"
    elif loud ==0:
        return 'Hello '+ name.lower()+"."
    else:
        return 0 

print(hello(name='Kim', loud=0))
print(hello(name='Kim'))








