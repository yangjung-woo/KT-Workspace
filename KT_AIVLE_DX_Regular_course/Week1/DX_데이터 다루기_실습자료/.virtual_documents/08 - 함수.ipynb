# 함수 만들기
def divide(a, b) :
    if b == 0 :
        print('나누는 값이 0이 될 수 없습니다.')
    elif (type(a) != int) or (type(b) != int):
        print('정수 연산만 가능합니다.')
    else:
        result = a / b
        return result

# 함수 사용
divide(10, 0)
divide(10, 'ab')
divide(10, 2)








